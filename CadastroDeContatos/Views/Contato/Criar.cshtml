@{
    ViewData["Title"] = "Cadastrar Contato";
}
<div class="text-center">

    <h1 class="display-6">Cadastrar Contato</h1>

</div>

<form>
    <div class="mb-3">
        <label for="nome" class="form-label">Nome</label>
        <input type="text" class="form-control" placeholder="Digite o nome do contato">
    </div>

    <div class="mb-3">
        <label for="email" class="form-label">E-mail</label>
        <input type="text" class="form-control" placeholder="Digite o e-mail do contato">
    </div>

    <div class="mb-3">
        <label for="celular" class="form-label">Celular</label>
        <input type="text" class="form-control" placeholder="Digite o celular do contato">
    </div>

    <button type="button" class="btn btn-outline-success">Cadastrar</button>
    <a class="btn btn-outline-secondary" asp-controller="Contato" asp-action="Index" role="button">Voltar</a>
</form>
